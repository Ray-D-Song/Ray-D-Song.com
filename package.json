{
	"name": "vite-vue3-simple-ssr",
	"version": "0.1.0",
	"description": "simple ssr starter",
	"author": "Ray-D-Song",
	"license": "ISC",
	"type": "module",
	"keywords": [
		"vue3",
		"koa",
		"ssr",
		"vite"
	],
	"scripts": {
		"dev:client": "vite",
		"dev": "npx swc ./server.ts -o ./dist/index.mjs && NODE_ENV=development node ./dist/index.mjs",
		"build:client": "vite build --ssrManifest",
		"build:server": "npx swc ./server.ts -o ./dist/index.mjs && vite build --ssr src/serverEntry.ts --outDir dist/server",
		"build": "npm run build:client && npm run build:server",
		"preview": "npm run build && NODE_ENV=production SERVER_PORT=9292 node ./dist/index.mjs",
		"test:unit": "vitest",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"@iconify-json/fluent": "^1.1.49",
		"@iconify-json/mdi": "^1.1.64",
		"@types/koa": "^2.15.0",
		"@types/koa-compress": "^4.0.6",
		"@types/koa-static": "^4.0.4",
		"@unocss/reset": "^0.58.5",
		"@unocss/transformer-directives": "^0.58.5",
		"@vueuse/core": "^10.9.0",
		"fast-glob": "^3.3.2",
		"feed": "^4.2.2",
		"gray-matter": "^4.0.3",
		"koa": "^2.15.0",
		"koa-compress": "^5.1.1",
		"koa-connect": "^2.1.0",
		"koa-static": "^5.0.0",
		"lodash": "^4.17.21",
		"markdown-it": "^14.0.0",
		"msw": "^2.2.2",
		"pinia": "^2.1.7",
		"pino": "^8.19.0",
		"shikiji": "^0.10.2",
		"unplugin-vue-markdown": "^0.25.2",
		"vconsole": "^3.15.1",
		"vue": "^3.4.21",
		"vue-router": "^4.3.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.6.4",
		"@iconify-json/grommet-icons": "^1.1.10",
		"@swc/cli": "^0.1.65",
		"@swc/core": "^1.4.2",
		"@tsconfig/node18": "^18.2.2",
		"@types/jsdom": "^21.1.6",
		"@types/lodash": "^4.14.202",
		"@types/markdown-it": "^13.0.7",
		"@types/node": "^18.19.21",
		"@vitejs/plugin-vue": "^4.6.2",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/test-utils": "^2.4.4",
		"@vue/tsconfig": "^0.5.1",
		"clipboard-polyfill": "^4.0.2",
		"eslint": "^8.57.0",
		"jsdom": "^23.2.0",
		"markdown-it-shikiji": "^0.10.2",
		"npm-run-all2": "^6.1.2",
		"shikiji-twoslash": "^0.10.2",
		"typescript": "~5.3.3",
		"unocss": "^0.58.5",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^5.1.4",
		"vitest": "^1.3.1",
		"vue-tsc": "^1.8.27"
	}
}
